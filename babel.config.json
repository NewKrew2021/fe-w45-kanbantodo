{
  "presets": [
    [
      "@babel/preset-env",
      { 
        "targets": "> 0.25%, not dead", 
        "corejs" : {"version": 3, "proposals": true }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-private-methods",
    "@babel/plugin-transform-async-to-generator",
    "@babel/plugin-transform-runtime"
  ]
}